-@workouts.each do |workout|
  .card
    .card-header
      .card-subtitle.text-gray
        =link_to workout.created_at.strftime("%a, %b %d %Y"), edit_workout_path(workout)
      .card-body.columns
        - workout.unique_exercises.each do |exercise|
          .col-6
            =exercise.name
          .col-4
            =current_user.previous_workout_weight(exercise.id)
          .col-2
            -workout.routine.try(:name)

