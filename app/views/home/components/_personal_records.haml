.card
  .card-header
    .card-title.h5
      Personal Records
    .card-body
      .columns.col-12
        -@prs.each do |pr|
          .col-6{style: "text-align: right; padding-right: 10px"}
            =pr.exercise.name
          .col-6{style: "text-align: left; padding-left: 10px"}
            ="#{pr.weight} x #{pr.reps_completed}"
